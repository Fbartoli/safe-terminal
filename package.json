{
	"name": "safe-terminal",
	"version": "0.0.1",
	"license": "MIT",
	"bin": {
		"safe-terminal": "dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"start": "node dist/cli.js",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@reown/appkit": "^1.6.9",
		"@safe-global/api-kit": "^2.5.11",
		"@safe-global/protocol-kit": "^5.2.4",
		"@safe-global/sdk-starter-kit": "^1.1.7",
		"@safe-global/types-kit": "1.0.4",
		"@types/qrcode-terminal": "^0.12.2",
		"@walletconnect/ethereum-provider": "^2.19.0",
		"dotenv": "^16.4.7",
		"ethers": "^6.13.5",
		"ink": "^4.4.1",
		"ink-spinner": "^5.0.0",
		"meow": "^11.0.0",
		"qrcode-terminal": "^0.12.0",
		"react": "^18.3.1",
		"react-devtools-core": "^6.1.1",
		"viem": "^2.23.7"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.3.18",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.3.1",
		"chalk": "^5.4.1",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^4.6.2",
		"ink-testing-library": "^3.0.0",
		"nodemon": "^3.1.9",
		"prettier": "^2.8.8",
		"prisma": "^6.4.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.2",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
